<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hospital.baronic.domain.user.UserMapper">

    <select id = "getAllPatientList" resultType="com.hospital.baronic.web.dto.UserDto">
        SELECT `id`
        , `passwd`
        , `salt`
        , `is_avlid`
        , `phone_num`
        FROM baronic_db.admin_user
        WHERE 1=1
        AND id = {id}
    </select>
</mapper>